#setup for archer
bar {
        # status_command i3status

  font pango:FontAwesome, CascadiaCode 11
  status_command /usr/bin/bumblebee-status -m wakatime mpd nic disk:root cpu \
    memory pasink pasource datetime  \ #bluetooth hidden
    -p interval=1 root.path=/ disk.format="{used}/{size}" \
    wakatime.token='waka_e4a718ea-92fd-4c31-b292-78ada5e004ca'\
    nic.format="{intf} {ip}" memory.format="{percent:05.02f}%" \
    -t dracula-powerline
}
